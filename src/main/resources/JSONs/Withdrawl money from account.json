{
  "testCases": [
    {
      "description": "Withdraw a positive number lower than the balance",
      "amount": 100,
      "expectedOutput": {
        "success": true
      }
    },
    {
      "description": "Withdraw a positive number greater than the balance",
      "amount": 2500,
      "expectedOutput": {
        "success": true
      }
    },
    {
      "description": "Withdraw a negative number",
      "amount": -500,
      "expectedOutput": {
        "success": false,
        "error": "Invalid amount"
      }
    },
    {
      "description": "Withdraw an empty value",
      "amount": "",
      "expectedOutput": {
        "success": false,
        "error": "Invalid amount"
      }
    }
  ]
}
