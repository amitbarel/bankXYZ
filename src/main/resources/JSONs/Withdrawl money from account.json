{
  "testCases": [
    {
      "description": "Withdraw a positive number lower than the balance",
      "input": {
        "amount": 100
      },
      "expectedOutput": {
        "success": true
      }
    },
    {
      "description": "Withdraw a positive number greater than the balance",
      "input": {
        "amount": 2000
      },
      "expectedOutput": {
        "success": true
      }
    },
    {
      "description": "Withdraw a negative number",
      "input": {
        "amount": -500
      },
      "expectedOutput": {
        "success": false,
        "error": "Invalid amount"
      }
    },
    {
      "description": "Withdraw an empty value",
      "input": {
        "amount": ""
      },
      "expectedOutput": {
        "success": false,
        "error": "Invalid amount"
      }
    },
    {
      "description": "Withdraw letters",
      "input": {
        "amount": "abc"
      },
      "expectedOutput": {
        "success": false,
        "error": "Invalid amount"
      }
    }
  ]
}
